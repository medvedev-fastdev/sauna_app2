$('.row').remove();
$('.empty-row').remove();
<% if !@sms_messages.empty? %>
	$('.digg_pagination').show();
	$('<%= render @sms_messages %>').appendTo($('.result-list'));
	selectPaginatorLink('/admin/sms_messages', <%= @current_page_number %>);
<% else %>
	$('.digg_pagination').hide();
	$("<%= render 'shared/empty_row' %>").appendTo($('.result-list'));
<% end %>
