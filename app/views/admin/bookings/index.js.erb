$('.row').remove();
$('.empty-row').remove();
<% if !@bookings.empty? %>
	$('<%= render @bookings %>').appendTo($('.result-list'));
	$('.row a').click(function(e) {	
		e.preventDefault();
		showBookingInfoPopupAdmin('#dialog-booking', this.id);
	});	
	$('.digg_pagination').show();
	selectPaginatorLink('/admin/bookings', <%= @current_page_number %>);
<% else %>
	$('.digg_pagination').hide();
	$("<%= render 'shared/empty_row' %>").appendTo($('.result-list'));
<% end %>	