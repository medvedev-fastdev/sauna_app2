			<h2><%= t :create_new_sauna_photos %></h2>
				<div class="_expanded">
					<%= form_for @sauna, :url => admin_sauna_photos_path(:id => @sauna.id), :html => { :multipart => true, :class => "comment-form" } do |f| %>					
